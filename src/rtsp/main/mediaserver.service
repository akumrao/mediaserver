[Unit]
Description=mediaserver
After=network.target
StartLimitIntervalSec=0
[Service]
Type=simple
Restart=always
RestartSec=1
User=root
ExecStart=/workspace/mediaserver/src/rtsp/main/mediaserver
WorkingDirectory=/workspace/mediaserver/src/rtsp/main
#ExecReload=/bin/kill -HUP $MAINPID
KillMode=process
Restart=on-failure

[Install]
WantedBy=multi-user.target
